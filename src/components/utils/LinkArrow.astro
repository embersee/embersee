---
interface Props {
  width?: number;
  height?: number;
}

const { width, height } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width={width || "16"}
  height={height || "16"}
  viewBox="0 0 24 24"
  fill="none"
  stroke="#e7e7d8"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  class="lucide lucide-arrow-up-right"
  ><path d="M7 7h10v10"></path><path d="M7 17 17 7"></path></svg
>
